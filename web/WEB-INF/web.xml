<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
    <servlet-name>LoginDataServlet</servlet-name>
    <servlet-class>ictgradschool.project.servlets.LoginDataServlet</servlet-class>
</servlet>

    <servlet-mapping>
        <servlet-name>LoginDataServlet</servlet-name>
        <url-pattern>/loginData</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginDataServletNew</servlet-name>
        <servlet-class>ictgradschool.project.servlets.LoginDataServletNew</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginDataServletNew</servlet-name>
        <url-pattern>/loginDataNew</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>ictgradschool.project.servlets.LogOutServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProfInfoServlet</servlet-name>
        <servlet-class>ictgradschool.project.servlets.ProfInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProfInfoServlet</servlet-name>
        <url-pattern>/userinfo</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>EditProfServlet</servlet-name>
        <servlet-class>ictgradschool.project.servlets.EditProfServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EditProfServlet</servlet-name>
        <url-pattern>/editProf</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangeIconServlet</servlet-name>
        <servlet-class>ictgradschool.project.servlets.ChangeIconServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ChangeIconServlet</servlet-name>
        <url-pattern>/changeicon</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <servlet-class>ictgradschool.project.servlets.DeleteAccountServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <url-pattern>/deleteAccount</url-pattern>
    </servlet-mapping>

</web-app>